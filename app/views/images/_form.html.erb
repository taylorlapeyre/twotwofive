<%= form_for(:image, remote: true, url: images_path) do |f| %>
	<% if @listing %>
		<%= f.hidden_field :listing_id, value: @listing.id %>
	<% elsif @floorplan %>
		<%= f.hidden_field :floorplan_id, value: @floorplan.id %>
	<% end %>

	<%= f.file_field :image_file %>
	<br>
	<%= f.submit "Upload" %>
<% end %>